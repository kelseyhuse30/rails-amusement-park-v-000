<% if current_user %>
	<li><%= link_to "See attractions", attractions_path, class: "navbar-item" %></li>
	<li><%= link_to "Log Out", signout_path, method: :delete, class: "navbar-item" %></li>
<% else %>
	<li><%= link_to "Sign In", signin_path, class: "navbar-item" %></li>
	<li><%= link_to "Sign Up", new_user_path, class: "navbar-item" %></li>
<% end %>